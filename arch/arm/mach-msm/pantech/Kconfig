	bool
	select PANTECH_BMS_UPDATE
	select PANTECH_BMS_BATTERY_TYPE
	select PANTECH_BMS_TEST

config PANTECH_BMS_UPDATE
	bool

config PANTECH_BMS_BATTERY_TYPE
	bool

